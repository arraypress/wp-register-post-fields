{
  "name": "arraypress/wp-register-post-fields",
  "version": "1.0.0",
  "description": "Lightweight library for registering custom metaboxes with fields on WordPress post edit screens.",
  "homepage": "https://github.com/arraypress/wp-register-post-fields/",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\RegisterPostFields\\": "src/"
    },
    "files": [
      "src/Functions.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-register-post-fields/issues",
    "source": "https://github.com/arraypress/wp-register-post-fields"
  },
  "keywords": [
    "wordpress",
    "metabox",
    "post-meta",
    "custom-fields",
    "admin",
    "repeater",
    "gallery"
  ]
}